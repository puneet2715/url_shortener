{
  "name": "url-shorty",
  "version": "1.0.0",
  "description": "Advanced URL Shortener with Analytics and Rate Limiting",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:unit": "jest src/tests/unit",
    "test:integration": "jest src/tests/integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "init-db": "node src/scripts/init-db.js"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/src/tests/setupJest.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/tests/**",
      "!src/docs/**",
      "!src/scripts/**"
    ],
    "coverageDirectory": "coverage",
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "geoip-lite": "^1.4.9",
    "helmet": "^7.1.0",
    "ioredis": "^5.5.0",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nanoid": "^3.3.7",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.11.3",
    "redis": "^4.6.13",
    "swagger-ui-express": "^5.0.0",
    "ua-parser-js": "^1.0.37",
    "validator": "^13.12.0",
    "winston": "^3.11.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "supertest": "^6.3.4"
  }
}
